                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element name="fkComunePozzo"  type="xs:integer" minOccurs="0" maxOccurs="1" />
					
					<xs:element name="istatComunePozzo" minOccurs="0" maxOccurs="1" >
