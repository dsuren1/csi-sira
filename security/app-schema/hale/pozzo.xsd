<?xml version="1.0"?>
<schema version="1.0" 
    xmlns="http://www.w3.org/2001/XMLSchema"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:gml="http://www.opengis.net/gml/3.2"
	xmlns:decsiraogc_operecaptazione="http://www.regione.piemonte.it/ambiente/decsiraogc_operecaptazione/1.0"
    targetNamespace="http://www.regione.piemonte.it/ambiente/decsiraogc_operecaptazione/1.0"
    elementFormDefault="qualified" attributeFormDefault="unqualified">
	
	<import namespace="http://www.opengis.net/gml/3.2" 
	schemaLocation="http://schemas.opengis.net/gml/3.2.1/gml.xsd" /> 
	
	
	<!-- elements -->
	<!-- here extend  gml:AbstractObject o gml:AbstractFeature -->
	
	<!-- decsira_geo_pozzo  id= id_pozzo-->
	<element name="Pozzo" type="decsiraogc_operecaptazione:PozzoType" substitutionGroup="gml:AbstractFeature" />
	
	<!-- decsira_t_filtro_pozzo  id= id_filtro_pozzo-->
	<element name="FiltroPozzo" type="decsiraogc_operecaptazione:FiltroPozzoType" substitutionGroup="gml:AbstractObject" />
	
	<!-- decsira_t_oggetto_associato_derivazioni  id= identificativo-->
	<element name="OggettoAssociatoPozzo" type="decsiraogc_operecaptazione:OggettoAssociatoPozzoType" substitutionGroup="gml:AbstractObject" />
	
	<!-- decsira_t_cementazione_pozzo  id= id_cementazione_pozzo-->
	<element name="CementazPozzo" type="decsiraogc_operecaptazione:CementazPozzoType" substitutionGroup="gml:AbstractObject" />
	
	<!-- decsira_t_diametro_colonna_pozzo  id= id_diamcolon_pozzo-->
	<element name="DiamColonPozzo" type="decsiraogc_operecaptazione:DiamColonPozzoType" substitutionGroup="gml:AbstractObject" />
	
	<!-- decsira_t_emungim_durata_pozzo  id= id_emungdurata_pozzo-->
	<element name="EmungDurataPozzo" type="decsiraogc_operecaptazione:EmungDurataPozzoType" substitutionGroup="gml:AbstractObject" />
	
	<!-- decsira_t_emungim_gradini_pozzo  id= id_emunggradini_pozzo-->
	<element name="EmungGradiniPozzo" type="decsiraogc_operecaptazione:EmungGradiniPozzoType" substitutionGroup="gml:AbstractObject" />
	
	<!-- decsira_t_dreno_pozzo  id= id_dreno_pozzo-->
	<element name="DrenoPozzo" type="decsiraogc_operecaptazione:DrenoPozzoType" substitutionGroup="gml:AbstractObject" />
	
	<!-- decsira_t_pompa_idraul_pozzo  id= id_pompaidr_pozzo-->
	<element name="PompaIdraulPozzo" type="decsiraogc_operecaptazione:PompaIdraulPozzoType" substitutionGroup="gml:AbstractObject" />
	
	<!-- decsira_t_prelievo_perutenza_captazione  id= id_prelievo_utenza_captazione-->
	<element name="PrelievoPerUtenzaPozzo" type="decsiraogc_operecaptazione:PrelievoPerUtenzaPozzoType" substitutionGroup="gml:AbstractObject" />
	
	<!-- decsira_t_derivazioni_captazioni  id= id_deriv_capt-->
	<element name="PozzoDerivazione" type="decsiraogc_operecaptazione:PozzoDerivazioneType" substitutionGroup="gml:AbstractObject" />
	

	
	<!-- types -->
	<!-- here extend gml:AbstractGMLType o gml:AbstractFeatureType -->
	
	<complexType name="PozzoType">
        <complexContent>
            <extension base="gml:AbstractFeatureType">
                <sequence>
				
