					<xs:element name="profonditaAFiltro" type="xs:decimal" minOccurs="0" maxOccurs="1" />
					
					<xs:element name="aperturaFiltro" type="xs:decimal" minOccurs="0" maxOccurs="1" />
				
                </sequence>
